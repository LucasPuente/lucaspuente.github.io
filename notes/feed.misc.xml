---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
	<channel>
		<title>{{Lucas Puente's Blog | xml_escape }} - Miscellaneous</title>
		<description>Posts categorized as 'Misc. Economics'</description>
		<link>{{http://lucaspuente.github.io}}</link>
		<atom:link href="{{http://lucaspuente.github.io}}/notes/feed.misc.xml" rel="self" type="application/rss+xml" />
		{% for post in site.categories.Misc. Economics limit:10 %}
			<item>
				<title>{{ post.title | xml_escape }}</title>
				{% if post.excerpt %}
					<description>{{ post.excerpt | xml_escape }}</description>
				{% else %}
					<description>{{ post.content | xml_escape }}</description>
				{% endif %}
				<pubDate>{{ post.date | date_to_rfc822 }}</pubDate>
				<link>{{http://lucaspuente.github.io}}{{ post.url }}</link>
				<guid isPermaLink="true">{{http://lucaspuente.github.io}}{{ post.url }}</guid>
			</item>
		{% endfor %}
	</channel>
</rss>
